# Copy this file to .env and fill in your actual values

# Contract Addresses
REACT_APP_FHEVM_CONTRACT_ADDRESS=YOUR_CONTRACT_ADDRESS_HERE
REACT_APP_ZAMA_STANDARD_CONTRACT_ADDRESS=YOUR_ZAMA_STANDARD_CONTRACT_ADDRESS
REACT_APP_ZAMA_FHEVM_STANDARD_CONTRACT_ADDRESS=YOUR_ZAMA_FHEVM_STANDARD_CONTRACT_ADDRESS
REACT_APP_EXECUTOR_CONTRACT=YOUR_EXECUTOR_CONTRACT_ADDRESS
REACT_APP_ACL_CONTRACT=YOUR_ACL_CONTRACT_ADDRESS
