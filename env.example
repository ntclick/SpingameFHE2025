# Network Configuration
REACT_APP_CHAIN_ID=11155111
REACT_APP_GATEWAY_CHAIN_ID=55815
REACT_APP_SEPOLIA_RPC_URL=YOUR_SEPOLIA_RPC_URL_HERE
