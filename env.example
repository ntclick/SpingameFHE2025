# Private Keys (⚠️ NEVER commit real private keys to git)
REACT_APP_PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE

# Game Configuration
REACT_APP_PRICE_PER_SPIN=0.01

# Development Settings
REACT_APP_STRICT_FHE_ONLY=true
