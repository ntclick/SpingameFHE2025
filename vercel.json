{
  "name": "SpingameFHE2025",
  "version": 2,
  "buildCommand": "cd frontend-fhe-spin && npm install && npm run build",
  "outputDirectory": "frontend-fhe-spin/build",
  "framework": "create-react-app",
  "installCommand": "npm install",
  "env": {
    "REACT_APP_FHEVM_CONTRACT_ADDRESS": "YOUR_CONTRACT_ADDRESS",
    "REACT_APP_SEPOLIA_RPC_URL": "YOUR_SEPOLIA_RPC_URL",
    "REACT_APP_GATEWAY_CHAIN_ID": "11155111",
    "REACT_APP_RELAYER_URL": "https://relayer.testnet.zama.cloud",
    "REACT_APP_ETHERSCAN_API_KEY": "YOUR_ETHERSCAN_API_KEY",
    "REACT_APP_CHAIN_ID": "11155111",
    "REACT_APP_GATEWAY_CHAIN_ID": "55815",
    "REACT_APP_PRICE_PER_SPIN": "0.01",
    "REACT_APP_STRICT_FHE_ONLY": "true",
    "REACT_APP_ZAMA_STANDARD_CONTRACT_ADDRESS": "YOUR_ZAMA_STANDARD_CONTRACT_ADDRESS",
    "REACT_APP_ZAMA_FHEVM_STANDARD_CONTRACT_ADDRESS": "YOUR_ZAMA_FHEVM_STANDARD_CONTRACT_ADDRESS",
    "REACT_APP_EXECUTOR_CONTRACT": "YOUR_EXECUTOR_CONTRACT",
    "REACT_APP_ACL_CONTRACT": "YOUR_ACL_CONTRACT",
    "REACT_APP_HCU_LIMIT_CONTRACT": "YOUR_HCU_LIMIT_CONTRACT",
    "REACT_APP_KMS_VERIFIER_CONTRACT": "YOUR_KMS_VERIFIER_CONTRACT",
    "REACT_APP_INPUT_VERIFIER_CONTRACT": "YOUR_INPUT_VERIFIER_CONTRACT",
    "REACT_APP_DECRYPTION_ORACLE_CONTRACT": "YOUR_DECRYPTION_ORACLE_CONTRACT",
    "REACT_APP_DECRYPTION_ADDRESS": "YOUR_DECRYPTION_ADDRESS",
    "REACT_APP_INPUT_VERIFICATION_ADDRESS": "YOUR_INPUT_VERIFICATION_ADDRESS"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_FHEVM_CONTRACT_ADDRESS": "YOUR_CONTRACT_ADDRESS",
      "REACT_APP_SEPOLIA_RPC_URL": "YOUR_SEPOLIA_RPC_URL",
      "REACT_APP_RELAYER_URL": "https://relayer.testnet.zama.cloud"
    }
  }
}
